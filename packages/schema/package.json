{
  "name": "@dollygrip/schema",
  "version": "0.0.5",
  "description": "The Dollygrip schema module.",
  "keywords": [
    "api",
    "graphql",
    "schema",
    "the movie db",
    "tmdb"
  ],
  "homepage": "https://github.com/dylanaubrey/dollygrip",
  "bugs": {
    "url": "https://github.com/dylanaubrey/dollygrip/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/dollygrip"
  },
  "license": "MIT",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "compile": "concurrently npm:compile:*",
    "compile-watch": "concurrently npm:compile-watch:*",
    "compile-watch:main": "yarn run compile:main --watch",
    "compile-watch:types": "yarn run compile:types --watch",
    "compile:main": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib/main --extensions \".ts\" --source-maps --copy-files --config-file ../../babel.config.js",
    "compile:module": "cross-env BABEL_ENV=module babel ./src --out-dir ./lib/module --extensions \".ts\" --source-maps --copy-files --config-file ../../babel.config.js",
    "compile:types": "yarn run graphql-codegen && npm run format-types && tsc --build",
    "format-types": "tslint --fix --project ../../tsconfig.json ./src/types.ts",
    "graphql-codegen": "graphql-codegen"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "graphql-import": "^0.7.1"
  },
  "peerDependencies": {
    "@babel/runtime": "< 8",
    "core-js": "< 4",
    "lodash": "< 5"
  },
  "devDependencies": {
    "graphql": "^14.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
