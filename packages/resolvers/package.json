{
  "name": "@dollygrip/resolvers",
  "version": "0.0.5",
  "description": "The Dollygrip resolvers module.",
  "keywords": [
    "api",
    "graphql",
    "schema",
    "the movie db",
    "tmdb"
  ],
  "homepage": "https://github.com/dylanaubrey/dollygrip",
  "bugs": {
    "url": "https://github.com/dylanaubrey/dollygrip/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/dollygrip"
  },
  "license": "MIT",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "compile": "concurrently npm:compile:*",
    "compile-watch": "concurrently npm:compile-watch:*",
    "compile-watch:main": "yarn run compile:main --watch",
    "compile-watch:types": "yarn run compile:types --watch",
    "compile:main": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib/main --extensions \".ts\" --source-maps --config-file ../../babel.config.js",
    "compile:module": "cross-env BABEL_ENV=module babel ./src --out-dir ./lib/module --extensions \".ts\" --source-maps --config-file ../../babel.config.js",
    "compile:types": "tsc --build"
  },
  "dependencies": {
    "@graphql-box/core": "^2.0.1",
    "@graphql-box/helpers": "^2.0.3",
    "@repodog/types": "^0.3.27",
    "getta": "^0.1.5",
    "type-fest": "^0.11.0"
  },
  "peerDependencies": {
    "@babel/runtime": "< 8",
    "@dollygrip/rest-client": "< 1",
    "@dollygrip/schema": "< 1",
    "core-js": "< 4",
    "graphql": "< 15",
    "lodash": "< 5"
  },
  "devDependencies": {
    "@cachemap/constants": "^3.0.3",
    "@cachemap/core": "^3.0.2",
    "@cachemap/map": "^3.0.2",
    "fetch-mock": "^9.1.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
