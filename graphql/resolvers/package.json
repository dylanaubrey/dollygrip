{
  "name": "@dollygrip/resolvers",
  "version": "0.0.5",
  "description": "The Dollygrip resolvers module.",
  "homepage": "https://github.com/dylanaubrey/dollygrip",
  "bugs": {
    "url": "https://github.com/dylanaubrey/dollygrip/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/dollygrip/graphql/resolvers"
  },
  "license": "MIT",
  "author": "Dylan Aubrey",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "del-cli ./dist && npm run compile",
    "compile": "concurrently npm:compile:*",
    "compile-watch": "concurrently npm:compile-watch:*",
    "compile-watch:main": "npm run compile:main -- --watch",
    "compile-watch:types": "npm run compile:types -- --watch",
    "compile:main": "JS_ENV=node MODULE_SYSTEM=cjs rollup -c ./rollup.config.js",
    "compile:module": "JS_ENV=node MODULE_SYSTEM=esm rollup -c ./rollup.config.js",
    "compile:types": "tsc --build",
    "test": "jest --logHeapUsage"
  },
  "dependencies": {
    "@dollygrip/rest-client": "0.0.5",
    "@dollygrip/schema": "0.0.5",
    "getta": "0.1.5",
    "graphql": "15.5.0"
  },
  "devDependencies": {
    "@cachemap/core": "3.0.2",
    "@cachemap/map": "3.0.2",
    "@dollygrip/babel-config": "0.0.1",
    "@dollygrip/jest-config": "0.0.1",
    "@dollygrip/rollup-config": "0.0.1",
    "@graphql-box/core": "2.0.1",
    "@graphql-box/helpers": "2.0.5",
    "concurrently": "6.1.0",
    "del-cli": "3.0.1",
    "fetch-mock": "9.11.0",
    "jest": "26.6.0",
    "rollup": "2.47.0"
  }
}
