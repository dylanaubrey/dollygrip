{
  "name": "dollygrip",
  "version": "0.0.5",
  "private": true,
  "description": "A movie information portal.",
  "homepage": "https://github.com/dylanaubrey/dollygrip",
  "bugs": {
    "url": "https://github.com/dylanaubrey/dollygrip/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dylanaubrey/dollygrip.git"
  },
  "license": "MIT",
  "author": "Dylan Aubrey",
  "scripts": {
    "bootstrap": "lerna bootstrap -- --legacy-peer-deps",
    "build": "npm run clean:libs && npm run compile",
    "clean": "npm run clean:packages && npm run clean:libs && npm run clean:repo",
    "clean:libs": "lerna exec --parallel -- del-cli ./lib",
    "clean:packages": "lerna exec --parallel -- del-cli ./node_modules",
    "clean:repo": "del-cli ./node_modules",
    "compile": "lerna run compile --stream",
    "compile-watch": "lerna run compile-watch --parallel",
    "compile:browser": "lerna run compile:browser --stream",
    "compile:main": "lerna run compile:main --stream",
    "compile:module": "lerna run compile:module --stream",
    "compile:types": "lerna run compile:types --stream",
    "cutoff": "cutoff-lerna",
    "cutoff:post-version": "npm run compile",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "publish-cutoff": "publish-lerna-cutoff",
    "test": "lerna run test --stream",
    "type-check": "tsc --noEmit",
    "validate": "npm run build && npm run lint && npm run type-check && npm run test",
    "wipe": "npm run wipe:packages && npm run wipe:repo",
    "wipe:packages": "lerna exec --parallel -- del-cli ./npm.lock ./node_modules",
    "wipe:repo": "del-cli ./npm.lock ./node_modules"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@dollygrip/babel-config": "file:./configs/babel-config",
    "@dollygrip/eslint-config": "file:./configs/eslint-config",
    "@dollygrip/jest-config": "file:./configs/jest-config",
    "@dollygrip/prettier-config": "file:./configs/prettier-config",
    "@dollygrip/ts-config": "file:./configs/ts-config",
    "@playlyfe/gql": "2.6.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "codecov": "3.6.5",
    "concurrently": "5.1.0",
    "cutoff": "0.2.8",
    "del-cli": "3.0.0",
    "eslint": "7.11.0",
    "husky": "4.2.3",
    "jest": "26.6.0",
    "lerna": "3.20.2",
    "markdownlint-cli": "0.22.0",
    "typescript": "4.1.5"
  }
}
