{
  "name": "dollygrip",
  "version": "0.0.5",
  "private": true,
  "description": "A graphql interface for The Movie Database (TMDb) rest API.",
  "keywords": [
    "api",
    "graphql",
    "schema",
    "the movie db",
    "tmdb"
  ],
  "homepage": "https://github.com/dylanaubrey/dollygrip",
  "bugs": {
    "url": "https://github.com/dylanaubrey/dollygrip/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dylanaubrey/dollygrip.git"
  },
  "license": "MIT",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn run clean:libs && yarn run compile",
    "clean": "yarn run clean:packages && yarn run clean:libs && yarn run clean:repo",
    "clean:libs": "lerna exec --parallel -- del-cli ./lib",
    "clean:packages": "lerna exec --parallel -- del-cli ./node_modules",
    "clean:repo": "del-cli ./node_modules",
    "compile": "lerna run compile --stream",
    "compile-watch": "lerna run compile-watch --parallel",
    "compile:browser": "lerna run compile:browser --stream",
    "compile:main": "lerna run compile:main --stream",
    "compile:module": "lerna run compile:module --stream",
    "compile:types": "lerna run compile:types --stream",
    "cutoff": "cutoff-lerna",
    "cutoff:post-version": "yarn run compile",
    "init": "yarn && yarn run bootstrap",
    "lint": "concurrently npm:lint:*",
    "lint:js": "eslint .",
    "lint:ts": "tslint --project ./tsconfig.json",
    "new-monorepo": "new-monorepo",
    "new-package": "new-package",
    "new-package:post": "yarn run bootstrap",
    "publish-cutoff": "publish-lerna-cutoff",
    "test": "jest --logHeapUsage",
    "type-check": "tsc --noEmit",
    "validate": "yarn run build && yarn run lint && yarn run type-check && yarn run test",
    "wipe": "yarn run wipe:packages && yarn run wipe:repo",
    "wipe:packages": "lerna exec --parallel -- del-cli ./yarn.lock ./node_modules",
    "wipe:repo": "del-cli ./yarn.lock ./node_modules"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-syntax-top-level-await": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@playlyfe/gql": "^2.6.2",
    "@repodog/babel-config": "^0.3.32",
    "@repodog/build-references": "^0.3.28",
    "@repodog/commitlint-config": "^0.3.25",
    "@repodog/config-helpers": "^0.3.31",
    "@repodog/constants": "^0.3.30",
    "@repodog/eslint-config": "^0.3.27",
    "@repodog/helpers": "^0.3.28",
    "@repodog/jest-config": "^0.3.32",
    "@repodog/lerna-config": "^0.3.25",
    "@repodog/markdownlint-config": "^0.3.25",
    "@repodog/new-monorepo": "^0.3.36",
    "@repodog/new-package": "^0.3.27",
    "@repodog/new-repo": "^0.3.41",
    "@repodog/prettier-config": "^0.3.26",
    "@repodog/ts-config": "^0.3.25",
    "@repodog/tslint-config": "^0.3.27",
    "@repodog/types": "^0.3.27",
    "@types/fetch-mock": "^7.3.2",
    "@types/fs-extra": "^8.1.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "codecov": "^3.6.5",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "cutoff": "^0.2.8",
    "del-cli": "^3.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-sort-class-members": "^1.6.0",
    "fetch-mock": "^9.0.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.6.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lerna": "^3.20.2",
    "lodash": "^4.17.15",
    "markdownlint-cli": "^0.22.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "ts-toolbelt": "^6.3.5",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "type-fest": "^0.11.0",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0"
  },
  "engines": {
    "node": "< 10.16.0"
  }
}
